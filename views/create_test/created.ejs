    <script>
         function shareTest(e){
            const id =e.parentNode.id;
            const title = e.parentNode.title;
            $('.modal-title').html(title);
            $('#url').val(document.location.origin+'/test/'+id);
            $('#shareTest').modal('show');
        }
            function delTest(e){
                let r = confirm("Bạn có chắc chắn muốn xoá đề thi này ?");
                if(r){
                    fetch('/api/test/'+e.parentNode.id,{
                            method: 'DELETE',
                        })
                        .then(function(response) {
                            location.reload();
                        })
                        .catch(err=>{
                            alert('Thất bại,thử lại sau');
                            console.log(err)
                        });
                }
            }
            function editTest(e){
                location.href='/test/'+e.parentNode.id+'/info';
            }
            function saveTest(e){
                fetch('/api/saved',{
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body:JSON.stringify({
                                id_user:document.getElementById('username').value,
                                id_test:e.parentNode.id
                            })
                        })
                        .then(function(response) {
                            alert('Lưu lại thành công');
                        })
                        .catch(err=>{
                            alert('Thất bại,thử lại sau');
                            console.log(err)
                    });
            }
        </script>
